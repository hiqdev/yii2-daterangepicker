    "require": {
        "bower-asset/bootstrap-daterangepicker": "^2.1 || ^3.0.1",
        "yiisoft/yii2": "*"
    },
    "require-dev": {
        "hiqdev/hidev-php": "<2.0 || dev-master",
        "hiqdev/hidev-hiqdev": "<2.0 || dev-master",
        "omnilight/yii2-momentjs": "*"
    },
    "autoload": {
        "psr-4": {
            "hiqdev\\yii2\\daterangepicker\\": "src",
            "hiqdev\\yii2\\daterangepicker\\tests\\": "tests"
        }
    },
    "extra": {
        "config-plugin": {
            "common": "src/config/common.php",
            "tests": "src/config/tests.php"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
